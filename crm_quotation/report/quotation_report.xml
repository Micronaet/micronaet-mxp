<?xml version="1.0"?>
<openerp>
<data noupdate="1">
    <record id="aeroo_crm_quotation_report_id" model="ir.actions.report.xml">
        <field name="name">CRM Quotation report</field>
        <field name="type">ir.actions.report.xml</field>
        <field name="model">sale.order</field>
        <field name="report_name">crm_quotation_report</field>
        <field name="report_type">aeroo</field>
        <field name="in_format">oo-odt</field>
        <field name="parser_loc">crm_quotation/report/quotation_parser.py</field>
        <field name="report_rml">crm_quotation/report/quotation.odt</field>
        <field name="parser_state">loc</field>
        <field name="tml_source">file</field>
        <field name="out_format" eval="ref('report_aeroo_ooo.report_mimetypes_pdf_odt')"></field>
    </record>

    <ir_set>
        <field eval="'action'" name="key"/>
        <field eval="'client_print_multi'" name="key2"/>
        <field eval="['sale.order']" name="models"/>
        <field name="name">crm_quotation_report_action</field>
        <field eval="'ir.actions.report.xml,'+str(aeroo_crm_quotation_report_id)" name="value"/>
        <field eval="True" name="isobject"/>
        <field eval="True" name="replace"/>
    </ir_set>
</data>
</openerp>

